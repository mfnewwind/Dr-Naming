div.table-line-title
  div.table-item.number.center
    span #
  div.table-item.width180
    span Name
  div.table-item.width180
    span location
  div.table-item.width100
    span Meaning
  div.table-item.width280
    span Comment
div.table-line(v-repeat='v : collections.classes')
  div.table-item.number.center
    span {{$index + 1}}
  div.table-item.width180.highlight
    span {{$key}}
  div.table-item.width180
    span {{v[0].path}}
  div.table-item.width100
    span Meaning
  div.table-item.width360
    span {{v[0].comment}}
  div.table-item.width80.highlight
    a Code View
