div.table-line(v-repeat='repos')
  a(v-attr='href: "/repo/" + repos[$index].owner + "/" + repos[$index].repo')
    div.table-item.number.center
      span {{$index + 1}}
    div.table-item.width180
      span {{repos[$index].repo}}
    div.table-item.width180
      span {{repos[$index].owner}}
    div.table-item.width180
      span {{now |date}}
    div.table-item.center.width60
      i.fa.icon(v-class='repos[$index].branches[0].files | statusCheck')
    div.table-item.arrow
      i.fa.fa-angle-right.icon
